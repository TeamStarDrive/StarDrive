  <ModuleType>Turret</ModuleType>
  <XSIZE>3</XSIZE>
  <YSIZE>3</YSIZE>
  <Name>Dual Missile Turret</Name>
  <Restrictions>O</Restrictions>
  <UID>MissileLauncher3x3</UID>
  <IconTexturePath>Modules/dualmissileturret_3x3</IconTexturePath>

  <NameIndex>996</NameIndex>
  <DescriptionIndex>997</DescriptionIndex>



  <Mass>10</Mass>
  <Health>900</Health>
  <HealthMax>900</HealthMax>
  <energyreq>0</energyreq>
