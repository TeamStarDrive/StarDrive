  <IconTexturePath>Modules/engine_3x3</IconTexturePath>
  <Mass>45</Mass>
  <Health>6080</Health>
  <HealthMax>6080</HealthMax>
  <energyreq>0</energyreq>
  
  <Description>This massive engine will provide awesome thrust at an awesome power cost.  </Description>
  
  
  <PowerDraw>75</PowerDraw>
	<PowerDrawAtWarp>-75</PowerDrawAtWarp>


  <NameIndex>1847</NameIndex>
  <DescriptionIndex>919</DescriptionIndex>

  <!-- for shield items -->
  
  <ShieldPowerMax>0</ShieldPowerMax>
  <ShieldRechargeDelay>0</ShieldRechargeDelay>
  <ShieldRechargeRate>0</ShieldRechargeRate>
  <ShieldRadius>0</ShieldRadius>

  <!-- for PowerPlant Items -->
  
  <PowerFlowMax>0</PowerFlowMax>
  <PowerStoreMax>0</PowerStoreMax>
  
  <!-- for Weapons -->

  
  
  <!-- for Engines -->
  <Thrust>135000</Thrust>
