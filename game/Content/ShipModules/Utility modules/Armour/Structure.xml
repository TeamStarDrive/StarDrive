<?xml version="1.0"?>
<ShipModule xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">


<!-- The Doctor, StarDrive Turbo: these are boolean values that configure precisely which ship hull classes are able to mount your module. Like the old 'FightersOnly' tag, except comprehensive in the control provided -->

  <DroneModule>false</DroneModule>

  
  <Cost>0.5</Cost>
  <!-- Tech Level -->
  <TechLevel>0</TechLevel>

  <!-- common traits shared by all ship modules -->
  <XSize>1</XSize>
  <YSize>1</YSize>
  <ModuleType>Armor</ModuleType>
  <Name>Structure</Name>
  <Restrictions>IO</Restrictions>
  <UID>Structure</UID> 
  <Version>2</Version>

  <TexturePath>Textures/transparent</TexturePath>
  <IconTexturePath>Modules/Structure</IconTexturePath>
  

  <NameIndex>18048</NameIndex>
  <DescriptionIndex>18049</DescriptionIndex>


  <!-- Mass and Health -->
  <Mass>1</Mass>
  <Health>300</Health>
  <HealthMax>300</HealthMax>
  <RepairDifficulty>0.5</RepairDifficulty>

</ShipModule>